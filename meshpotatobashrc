#MeshPotato
export MESHPOTATO_DIR=$HOME/projects/MeshPotato
export LD_LIBRARY_PATH=$MESHPOTATO_DIR/lib:$LD_LIBRARY_PATH
export CPATH=$MESHPOTATO_DIR/include:$CPATH
export PATH=$MESHPOTATO_DIR/bin:$PATH
export MESHPOTATO_PLUGIN_PATH=$MESHPOTATO_DIR/plugins
export PYTHONPATH=$MESHPOTATO_DIR:$PYTHONPATH


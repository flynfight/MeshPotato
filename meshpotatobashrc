#MeshPotato
export MESHPOTATO_DIR=$HOME/projects/MeshPotato
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$MESHPOTATO_DIR/lib
export CPATH=$CPATH:$MESHPOTATO_DIR/include
export PATH=$PATH:$MESHPOTATO_DIR/bin
export MESHPOTATO_PLUGIN_PATH=$MESHPOTATO_DIR/plugins
export PYTHONPATH=$PYTHONPATH:$MESHPOTATO_DIR

